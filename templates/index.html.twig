<!DOCTYPE html>
<html lang="{{ lang }}">
<head>
  <meta charset="UTF-8">
  <meta keywords="{{ keywords }}">
  <meta description="{{ desc }}">
  <title>{% block title %}  {% trans from "messages" %}
      messages.goodmorning
      {% endtrans %}{% endblock %}</title>
  {% block stylesheets %}
<link href="css/main.css" rel="stylesheet">
  {% endblock %}
</head>
<body>
  <h1>
  {% trans from "messages" %}
  messages.goodmorning
  {% endtrans %}
</h1>
  <br>
  <br>
  {% for country in countries %}
  <a href="//{{country.country}}.symfony4.challenge"><button>{{country.country|upper}}</button></a>
  {% endfor %}
  {% block body %}{% endblock %}
  {% block javascripts %}{% endblock %}
</body>
</html>
